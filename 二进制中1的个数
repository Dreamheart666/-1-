#include <stdio.h>
#include <windows.h>
#include<string.h>
#include <math.h>
#include<windows.h>
int coumun(unsigned int n)
{
	int count = 0;
	while (n)
	{
		if (n % 2 == 1)
		{
			count++;
		}
		n = n / 2;
	}

	return count;
}
int main()
{
	int a = 0;
	scanf("%d", &a);
	int count = coumun(a);
	printf("count=%d\n", count);
	return 0;;
}
